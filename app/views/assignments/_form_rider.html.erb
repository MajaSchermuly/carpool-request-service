<p> Note: Enter the information exactly as it was on the request form </p>

<%= form_tag(search_path, method: :get) do %>
  <div class="row">
    <%= label_tag "Enter your name:" %>
	<%= text_field_tag(:search_name, params[:search_name]) %>
  </div>
		
  <div class="row">
    <%= label_tag "Enter your phone number:" %>
    <%= text_field_tag(:search_phone_number, params[:search_phone_number]) %>
  </div>
		
  <%= submit_tag 'View Personal Queue', class: "grey wide button small-margins" %>
<% end %>